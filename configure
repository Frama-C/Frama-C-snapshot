then
   echo "${ECHO_T}"
   { { $as_echo "$as_me:$LINENO: error: unsupported version; GNU Make version 3.81
                 or higher is required." >&5
$as_echo "$as_me: error: unsupported version; GNU Make version 3.81
                 or higher is required." >&2;}
   { (exit 1); exit 1; }; };
else
   echo "${ECHO_T}Good!"
fi

