# Summary #
###########

new_section "summary: plug-ins available"

for plugin in  CONSTANT_PROPAGATION  FROM  GUI  IMPACT  INOUT  LTL_TO_ACSL  METRICS  MIEL  OCCURRENCE  PDG  POSTDOMINATORS  SCOPE  SECURITY  SEMANTIC_CALLGRAPH  SLICING  SPARECODE  SYNTACTIC_CALLGRAPH  USERS  VALUE  WP ; do
  n=NAME_$plugin
  e=ENABLE_$plugin
  d=DYNAMIC_$plugin
  i=INFO_$plugin
  eval nv=\$$n
  eval ev=\$$e
  eval dv=\$$d
  eval iv=\$$i
  if test "$ev" != "no" -a "$dv" = "yes"; then
    res="$ev, dynamic";
  else
    res=$ev;
  fi
  { $as_echo "$as_me:$LINENO: $nv: $res$iv" >&5
$as_echo "$as_me: $nv: $res$iv" >&6;}
done

if test "$EXTRA_EXTERNAL_PLUGINS" != ""; then
  new_section "summary: requested external plugins"
fi

for plugin in $EXTRA_EXTERNAL_PLUGINS; do
    { $as_echo "$as_me:$LINENO: $plugin" >&5
$as_echo "$as_me: $plugin" >&6;}
done
