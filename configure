for plugin in  CALLGRAPH SEMANTIC_CONSTANT_FOLDING FROM_ANALYSIS GUI IMPACT INOUT METRICS OCCURRENCE PDG POSTDOMINATORS RTEGEN SCOPE SLICING SPARECODE USERS VALUE_ANALYSIS AORAI E_ACSL LOOP_ANALYSIS NONTERM OBFUSCATOR PRINT_API QED REPORT SECURITY_SLICING STUDIA VARIADIC WP; do
  n=NAME_$plugin
  e=ENABLE_$plugin
  i=INFO_$plugin
  eval nv=\$$n
  eval ev=\$$e
  eval iv=\$$i
  { $as_echo "$as_me:${as_lineno-$LINENO}: $nv: $ev$iv" >&5
$as_echo "$as_me: $nv: $ev$iv" >&6;}
done

