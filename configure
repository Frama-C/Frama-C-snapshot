upper() {
  echo "$1" | tr "a-z-" "A-Z_"
}

lower() {
  echo "$1" | tr "A-Z_" "a-z-"
}



new_section() {
  banner=`echo "* $1 *" | sed -e 's/./*/g'`
  title=`echo "* $1 *" | tr "a-z" "A-Z"`
  { $as_echo "$as_me:$LINENO: $banner" >&5
$as_echo "$as_me: $banner" >&6;}
  { $as_echo "$as_me:$LINENO: $title" >&5
$as_echo "$as_me: $title" >&6;}
  { $as_echo "$as_me:$LINENO: $banner" >&5
$as_echo "$as_me: $banner" >&6;}
}



