  as_fn_error $? "Cannot find ocamlgraph via ocamlfind \
(requires ocamlgraph 1.8.5 or higher)." "$LINENO" 5
fi
case $OCAMLGRAPH in
      0.* | 1.[01234567].* \
      | 1.8.0 | 1.8.0+dev \
      | 1.8.1 | 1.8.1+dev \
      | 1.8.2 | 1.8.2+dev \
      | 1.8.3 | 1.8.3+dev \
      | 1.8.4 | 1.8.4+dev)
        as_fn_error $? "found $OCAMLGRAPH: requires 1.8.5 or higher." "$LINENO" 5;;
      1.8.5 | 1.8.6 | 1.8.7)
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: found" >&5
$as_echo "found" >&6; };;
      *)
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: found $OCAMLGRAPH: should work" >&5
$as_echo "found $OCAMLGRAPH: should work" >&6; };;
esac

